
entity UserProfile {
profileId Integer
username String
password String
}

entity Document {
documentId Integer
title String
content String
}

entity Images{
imageId Integer
documentIndex Integer
imageData String
caption String
}


relationship OneToMany{
Document{Images} to Images{Document}
}

relationship ManyToOne{
Document to User
}

relationship OneToOne{
UserProfile{User} to User
}